{
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "**/node_modules",
    "**/Pods"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@const/*": [
        "src/const/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@screens": [
        "src/screens"
      ],
      "@store/*": [
        "src/store/*"
      ],
    }
  }
}